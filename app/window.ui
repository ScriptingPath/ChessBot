  <property name="styleSheet">
   <string notr="true">/* Basic */

QWidget {
    background-color: rgb(23, 23, 23);
    color: #fff;
    border-radius: 9px;
}

QMainWindow &gt; QWidget {
    background-color: rgb(20, 20, 20) !important;
    color: #fff !important;
}

QPushButton,
QToolButton,
QCheckBox {
    background-color: rgb(27, 27, 27);
    border-radius: 5px;
    outline: none;
}

QComboBox,
QSpinBox,
QDoubleSpinBox,
QTimeEdit,
QDateEdit,
QDateTimeEdit,
QLineEdit,
QTextEdit,
QPlainTextEdit,
QKeySequenceEdit {
    background-color: rgb(28, 28, 28);
    border-radius: 5px;
    outline: none;
}


QPushButton:hover,
QToolButton:hover,
QRadioButton:hover,
QCheckBox:hover,
QComboBox:hover,
QLineEdit:hover,

QPlainTextEdit:hover,
QSpinBox:hover,
QDoubleSpinBox:hover,
QTimeEdit:hover,
QDateEdit:hover,
QDateTimeEdit:hover,
QKeySequenceEdit:hover {
    background-color: rgb(32, 32, 32);
}

QTextEdit:hover {
    background-color: rgb(25, 25, 25);
}

QPushButton:pressed,
QToolButton:pressed,
QRadioButton:pressed,
QCheckBox:pressed,
QComboBox:pressed,
QLineEdit:pressed,
QTextEdit:pressed,
QPlainTextEdit:pressed,
QSpinBox:pressed,
QDoubleSpinBox:pressed,
QTimeEdit:pressed,
QDateEdit:pressed,
QDateTimeEdit:pressed,
QKeySequenceEdit:pressed {
    background-color: rgb(36, 36, 36);
}

QLineEdit:focus,
QPlainTextEdit:focus,
QSpinBox:focus,
QDoubleSpinBox:focus,
QTimeEdit:focus,
QDateEdit:focus,
QDateTimeEdit:focus,
QKeySequenceEdit:focus {
    background-color: rgb(30.5, 30.5, 30.5)
}

QTextEdit:focus {
    background-color: rgb(28.5, 28.5, 28.5)
}

QComboBox {
    padding: 1px 0px 1px 3px;
}


QComboBox::drop-down
{
    border: 0px; /* This seems to replace the whole arrow of the combo box */
}

QComboBox::down-arrow {
    width: 199px;
    height: 14px;
}

QRadioButton {
    background-color: rgb(28, 28, 28);
    border-radius: 5px;
}

/* For Frames */

QFrame {
    background-color: rgb(23, 23, 23);
    border-radius: 9px;
}

QFrame &gt; QPushButton,
QFrame &gt; QPushButton,
QFrame &gt; QToolButton,
QFrame &gt; QCheckBox {
    background-color: rgb(30, 30, 30);
}

QFrame &gt; QComboBox,
QFrame &gt; QSpinBox,
QFrame &gt; QDoubleSpinBox,
QFrame &gt; QTimeEdit,
QFrame &gt; QDateEdit,
QFrame &gt; QDateTimeEdit,
QFrame &gt; QLineEdit,
QFrame &gt; QTextEdit,
QFrame &gt; QPlainTextEdit,
QFrame &gt; QKeySequenceEdit {
    background-color: rgb(31, 31, 31);
    border-radius: 5px;
    outline: none;
}


QFrame &gt; QPushButton:hover,
QFrame &gt; QToolButton:hover,
QFrame &gt; QRadioButton:hover,
QFrame &gt; QCheckBox:hover,
QFrame &gt; QComboBox:hover,
QFrame &gt; QLineEdit:hover,
QFrame &gt; QTextEdit:hover,
QFrame &gt; QPlainTextEdit:hover,
QFrame &gt; QSpinBox:hover,
QFrame &gt; QDoubleSpinBox:hover,
QFrame &gt; QTimeEdit:hover,
QFrame &gt; QDateEdit:hover,
QFrame &gt; QDateTimeEdit:hover,
QFrame &gt; QKeySequenceEdit:hover {
    background-color: rgb(35, 35, 35);
}

QFrame &gt; QPushButton:pressed,
QFrame &gt; QToolButton:pressed,
QFrame &gt; QRadioButton:pressed,
QFrame &gt; QCheckBox:pressed,
QFrame &gt; QComboBox:pressed,
QFrame &gt; QLineEdit:pressed,
QFrame &gt; QTextEdit:pressed,
QFrame &gt; QPlainTextEdit:pressed,
QFrame &gt; QSpinBox:pressed,
QFrame &gt; QDoubleSpinBox:pressed,
QFrame &gt; QTimeEdit:pressed,
QFrame &gt; QDateEdit:pressed,
QFrame &gt; QDateTimeEdit:pressed,
QFrame &gt; QKeySequenceEdit:pressed {
    background-color: rgb(39, 39, 39);
}

/* Vertical Scroll Bar */

QScrollBar:vertical {
    background-color: rgba(25, 25, 25, 0);
    border: 8px solid rgba(25, 25, 25, 0);
    width: 7px;
    max-width: 10px;
    min-width: 5px;
    margin: 15px 0 15px 0;
    border-radius: 7px !important;
    transition: 3s;
}

QScrollBar:vertical:hover {
    background-color: rgba(25, 25, 25, 0.830);
}

QScrollBar::handle:vertical {
    background-color: rgb(30, 30, 30);
    min-height: 30px;
    border-radius: 7px !important;
}
QScrollBar::handle:vertical:hover{
	background-color: rgb(32, 32, 32);
}

QScrollBar::handle:vertical:pressed {
	background-color: rgb(35, 35, 35);
}

/* Top Button */

QScrollBar::sub-line:vertical {
	border: none;
	background-color: transparent;
	height: 0px;
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
    width: 0px;
}

/* Bottom Button */

QScrollBar::add-line:vertical {
	border: none;
	background-color: transparent;
	height: 0px;
	border-top-left-radius: 0px;
	border-top-right-radius: 0px;
    width: 0px;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
	background: none;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}</string>
